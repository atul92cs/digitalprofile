 <header id="main-header">
    <div class="container">
       <h1>Your Own Digital profile</h1>
    </div>
 </header>
<div id="main-body">
   <div class="user">
        <table>
                 <tr>     
                 <th>Id</th>
                 <th>Name</th>
                 <th>Highschool</th>
                 <th>Senior secondary</th>
                 <th>College</th>
                 <th>Branch</th>
                 <th>Action</th>
                </tr>
             {{#each user}}
                <tr>
                    <td>{{this.id}}</td>
                    <td>{{this.Name}}</td>
                    <td>{{this.Highschool}}</td>
                    <td>{{this.Seniorsecondary}}</td>
                    <td>{{this.College}}</td>
                    <td>{{this.Branch}}</td>
                    <td><button onclick="deleteUser({{this.id}})">Delete User</button> <a href='/getuser/{{this.id}}'><button>View profile</button></a><a href='/updateprofile/{{this.id}}'><button>Edit Profile</button></a></td>
                </tr>
             {{/each}}
         </table>
   </div>
</div>
<footer id="footer">
   <p>&copy; 2020 Digital profile</p>
</footer>